{% extends '::base.html.twig' %}

{% block body %}
    <h1>Vos QCMs</h1>
    <table class="table table-striped">
        <tr>
            <th>Nom</th>
            <th>Theme</th>
            <th>Note</th>
        </tr>
        {% for qcm in qcms %}
        <tr>
            <td><a href="">{{ qcm.label }}</a></td>
            <td>{{ qcm.theme.label }}</td>
            <td>{{ qcm.note(app.user) }}</td>
        </tr>
        {% endfor %}
    </table>
{% endblock %}
